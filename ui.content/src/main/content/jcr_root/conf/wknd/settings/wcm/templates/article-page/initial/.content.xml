                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wknd/components/container">
                    <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wknd/components/title"/>
                    <title_1403531542
                            jcr:primaryType="nt:unstructured"
                            jcr:title="By author"
                            sling:resourceType="wknd/components/title"
                            linkTarget="_self"
                            type="h4"/>
                    <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wknd/components/text"/>
                </container>
            </container>
        </root>
        <cq:featuredimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="core/wcm/components/image/v3/image"
                altValueFromDAM="false"/>
